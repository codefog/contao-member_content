{
    "name": "codefog/contao-member_content",
    "description": "member_content extension for Contao Open Source CMS",
    "keywords": ["contao", "member", "content"],
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Codefog",
            "homepage": "https://codefog.pl"
        }
    ],
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^5.3"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "terminal42/contao-build-tools": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Codefog\\MemberContentBundle\\": "src"
        }
    },
    "extra": {
        "contao-manager-plugin": "Codefog\\MemberContentBundle\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "terminal42/contao-build-tools": true
        }
    }
}
